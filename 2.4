using System;

namespace _24_sol
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] arr = new int[] { 1, 2, 3, 4, 5, 6, 7, 8, 9 };
            Random rng = new Random();
            int n = arr.Length;
            while (n > 1)
            {
                int k = rng.Next(n--);
                int temp = arr[n];
                arr[n] = arr[k];
                arr[k] = temp;
            }
            Console.WriteLine(String.Join(",", arr));
        }
    }
}
