using System

namespace _6_sol
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n, count = 0;
            n = Convert.ToInt32(Console.ReadLine());
            for (int i = 2; i < n / 2 + 1; i++)
            {
                if (n % i == 0)
                    count++;
            }
            Console.WriteLine(count);
        }
    }
}
