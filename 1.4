using System;

namespace _4_sol
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int a, check = 0;
            a = Convert.ToInt32(Console.ReadLine());
            while (a != 0)
            {
                if (a % 10 > check)
                    check = a % 10;
                a = a / 10;
            }
            Console.WriteLine(check);
        }
    }
}
